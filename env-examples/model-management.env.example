# Model Management Service Environment Variables
# Copy this to system-services/model-management/.env

POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DB=smartaihub_models
SERVICE_PORT=8011
SERVICE_NAME=model-management
MODELS_DIR=models
MAX_MODEL_SIZE_MB=500
ALLOWED_MODEL_FORMATS=pkl,h5,joblib,onnx
DEFAULT_VERSION_SCHEME=semantic
AUTO_VERSION_INCREMENT=true
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001
LOG_LEVEL=INFO

